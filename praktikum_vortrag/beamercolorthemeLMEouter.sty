\ProvidesPackage{beamercolorthemeLMEouter}

\mode<presentation>

\setbeamercolor*{palette primary}{use=structure,fg=white,bg=structure.fg} % LME Blau
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=structure.fg!75!white} % Mittelblau
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=structure.fg!50!white} % Hellblau
\setbeamercolor*{palette quaternary}{use=structure,fg=white,bg=structure.fg!25!white} % Blassblau

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\setbeamercolor*{author in head/foot}{fg=LMEblue,bg=}
\setbeamercolor*{title in head/foot}{fg=LMEblue,bg=}
\setbeamercolor*{date in head/foot}{fg=white,bg=LMEblue}

\setbeamercolor*{frametitle}{fg=LMEblue,bg=LMEgrey}

\mode
<all>
